  <string name="share_app_dialog_btn_share">Chia sẻ</string>
  <string name="share_app_promotion_text">Này, hãy xem trình duyệt nhanh và nhẹ này từ %3$s. %1$s %2$s</string>
  <string name="your_rights_content1">%1$s là phần mềm nguồn mở và miễn phí được tạo bởi %2$s và những người đóng góp khác.</string>
  <string name="your_rights_content2"><![CDATA["%1$s được cung cấp cho bạn theo các điều khoản của "<a xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2" href="%2$s">%3$s</a>" và các giấy phép nguồn mở khác."]]></string>
  <string name="your_rights_content3"><![CDATA["Bạn không được cấp bất kỳ quyền hoặc giấy phép nào cho các nhãn hiệu của Tổ chức Mozilla hoặc bất kỳ bên nào, bao gồm %3$s, %4$s hoặc %1$s tên hoặc logo. Thông tin bổ sung có thể được tìm thấy "<a xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2" href="%2$s">ta&#803;i &#273;&#226;y</a>.]]></string>
